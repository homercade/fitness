                      i.fa.fa-edit
                      |  Action
                tbody
                  each br in intb
                    tr
                      td.hidden-phone  #{br.userfname} #{br.userlname}
                      td #{br.memclassname}-#{br.membershipname} 
                      td #{br.expirydate}  
                      td
