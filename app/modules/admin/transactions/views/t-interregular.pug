script.
  $(document).ready(function(){

    var date = new Date()
    date = moment(date).format('MMM DD, YYYY')
    var sub = false
    
    $('#reg').click(function(e){
     
      $('.or').modal('show')
      $('#name').text(`${$('#fname').val()} ${$('#lname').val()}`)
      $('#membership').text($("#memb option:selected").data('membership'))
      $('#address').text($('#addr').val())
      $('#email').text($('#emai').val())
      $('#contact').text($('#cont').val())
      $('#date').text(date)
      $('#quantity').text('-')
      $('#desc').text(`Membership: ${$("#memb option:selected").data('membership')}`)
      $('.total').text($("#memb option:selected").data('memfee'))
      $('#add').modal('dismiss')    
    })

   
    $(document).on("submit", "#form", function(e){  
      if(sub == false) {
        e.preventDefault()
        return  false
      }
      else {
        if ( $('.username').hasClass('inv') || $('.email').hasClass('inv') || sub === false ){ 
          e.preventDefault()
          return  false
        }
        //- else {
        //-   setTimeout(
        //-     continue, 2000
        //-   )
        //- }
      }
    })
  })


