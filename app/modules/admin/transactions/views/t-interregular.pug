script.
  $('.sorted').on('show.bs.modal', function(event){
    var button = $(event.relatedTarget)      
    var id = button.data('id')
    var userid = button.data('userid')
    let modal = $(this);     
    modal.find('.modal-body #id').val(` ${id}`)
    modal.find('.modal-body #userid').val(`${userid}`)
  })

